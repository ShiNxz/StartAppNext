{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noEmit": true,
        "baseUrl": ".",
        "jsx": "react",
        //"checkJs": true,
        "paths": {
            "@/pages/*": ["pages/*"],
            "@/components/*": ["components/*"],
            "@/UI/*": ["components/UI/*"],
            "@/data/*": ["utils/data/*"],
            "@/utils/*": ["utils/*"],
            "@/func/*": ["utils/functions*"],
            "@/models/*": ["utils/models/*"],
            "@/public/*": ["public/*"]
        }
    },
    "exclude": ["node_modules"]
}